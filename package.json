                "path": "./snippets/constants.json"
            },
            {
                "language": "java",
                "path": "./snippets/exceptions.json"
            },
            {
                "language": "java",
                "path": "./snippets/fields.json"
            },
            {
                "language": "java",
                "path": "./snippets/keywords.json"
            },
            {
                "language": "java",
                "path": "./snippets/lambdas.json"
            },
            {
                "language": "java",
                "path": "./snippets/loops.json"
            },
            {
                "language": "java",
                "path": "./snippets/methods.json"
            },
            {
                "language": "java",
                "path": "./snippets/object.json"
            },
            {
                "language": "java",
                "path": "./snippets/prints.json"
            },
            {
                "language": "java",
                "path": "./snippets/returns.json"
            },
            {
                "language": "java",
                "path": "./snippets/variables.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seudev/vscode-java-snippets.git"
    },
    "bugs": {
        "url": "https://github.com/seudev/vscode-java-snippets/issues"
    },
    "keywords": [
        "snippet",
        "vscode",
        "java",
        "Seudev"
    ],
    "scripts": {
        "update-package": "node src/PackageUpdater.js",
        "build-readme": "node src/ReadmeBuilder.js",
        "build": "npm run build-readme && npm run update-package",
        "prepublishOnly": "npm run build",
        "publish": "vsce publish -p $ACCESS_TOKEN"
    }
}
